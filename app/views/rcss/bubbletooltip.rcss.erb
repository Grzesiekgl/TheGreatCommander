.bubble_tooltip{
	width:147px;
	position:absolute;
	display:none;
        z-index: 100;
}
.bubble_tooltip .bubble_top{
	background-image: url('../images/tooltip/bubble_top1.PNG');
	background-repeat:no-repeat;
	height:16px;	
}
.bubble_tooltip .bubble_middle{
	background-image: url('../images/tooltip/bubble_middle1.PNG');
	background-repeat:repeat-y;	
	background-position:bottom left;
	padding-left:7px;
	padding-right:7px;
}
.bubble_tooltip .bubble_middle span{
	position:relative;
	top:-8px;
	font-family: Trebuchet MS, Lucida Sans Unicode, Arial, sans-serif;
	font-size:9px;
}
.bubble_tooltip .bubble_bottom{
	background-image: url('../images/tooltip/bubble_bottom1.PNG');
	background-repeat:no-repeat;
	background-repeat:no-repeat;	
	height:40px;
	position:relative;
	top:-6px;
}
